{
  "expo": {
    "name": "alitnest",
    "slug": "alitnest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/SplashIcon.png",
    "scheme": "alitnest",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/SplashIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFAFA"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SplashIcon.png",
        "backgroundColor": "#FAFAFA"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
